{
  "name": "Cantonese Pronunciation",
  "version": "0.1",
  "description": "A simple extension that allow you to check the definition and pronunciation of cantonese words",
  "permissions": ["*://humanum.arts.cuhk.edu.hk/*", "tabs"],
  "options_page": "options.html",
  "background": {
      "scripts": ["background.js"]
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "all_frames": true,
          "js": ["contentscript.js"]
      }
  ],
  "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "images/icon16.png",
          "32": "images/icon32.png",
          "48": "images/icon48.png",
          "128": "images/icon128.png"
      }
  },
  "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
  },
  "manifest_version": 2
}
